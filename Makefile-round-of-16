all: sim2d_2_8th_1 sim2d_2_8th_2 sim2d_2_8th_3 sim2d_2_8th_4 sim2d_2_8th_5 sim2d_2_8th_6 sim2d_2_8th_7 sim2d_2_8th_8

sim2d_2_8th_1:
	bash -c "MATCH_ID=sim2d_2_8th_1 HALF_ID=0 TEAM_B=023 TEAM_Y=014 ./run-docker.sh"
	bash -c "MATCH_ID=sim2d_2_8th_1 HALF_ID=1 TEAM_B=014 TEAM_Y=023 TEAM_B_INITIAL_SCORE=$$(./get_score.py ~/webots_video_out/ sim2d_2_8th_1 yellow) TEAM_Y_INITIAL_SCORE=$$(./get_score.py ~/webots_video_out/ sim2d_2_8th_1 blue) ./run-docker.sh"
